
    {{#each field as fieldItem}}

      <!-- Square card -->
      <div class="mdl-card mdl-shadow--2dp demo-card-square">
        <div class="mdl-card__title mdl-card--expand" style="background-image: url({{fieldItem.image}})
">
          <h2 class="mdl-card__title-text">{{fieldItem.name}}</h2>
        </div>
        <div class="mdl-card__supporting-text">
          {{fieldItem.shortdesc}}
        </div>
        <div class="mdl-card__actions mdl-card--border">
              <Link
                to="/{{fieldItem.name}}"
                className="mdl-button mdl-button--accent mdl-js-button mdl-js-ripple-effect"
              >
                More..
              </Link>
        </div>
      </div>

    {{/each}}
